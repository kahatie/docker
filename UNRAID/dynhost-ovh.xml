<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Beta>False</Beta>
<Category>Network:DNS</Category>
  <Name>Kahatie-DynHost-Ovh</Name>
  <Description>
    DynHost OVH Docker
    
    docker run \
    --name dynhost-ovh.my-domain \
    --restart=always \
    --detach \
    -e DYNHOST_DOMAIN_NAME=my-domain \
    -e DYNHOST_LOGIN=my-dynhost-user-set-on-ovh-manager \
    -e DYNHOST_PASSWORD=my-dynhost-password-set-on-ovh-manager \
    dynhost-ovh
  </Description>
  <Overview>
    DynHost OVH Docker
  </Overview>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/kahatie/dynhost-ovh.docker/</Registry>
  <GitHub>https://github.com/kahatie/dynhost-ovh.docker</GitHub>
  <Repository>kahatie/dynhost-ovh.docker</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>host</Mode>
    <Publish>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>DYNHOST_DOMAIN_NAME</Name>
      <Value>my-domain</Value>
    </Variable>
    <Variable>
      <Name>DYNHOST_LOGIN</Name>
      <Value>my-dynhost-user-set-on-ovh-manager</Value>
    </Variable>
    <Variable>
      <Name>DYNHOST_PASSWORD</Name>
      <Value>my-dynhost-password-set-on-ovh-manager</Value>
    </Variable>
  </Environment>
  <WebUI></WebUI>
</Containers>
